{
  pkgs,
  flakey-profile,
  ...
}:
flakey-profile.lib.mkProfile {
  inherit pkgs;
  paths = with pkgs; [
    {{- range .homepkgs.shared }}
    {{ . }}
    {{- end }}
    {{- if .work }}
    {{- range .homepkgs.work }}
    {{ . }}
    {{- end }}
    {{- end }}
    {{- if .devMachine }}
    {{- range .homepkgs.dev }}
    {{ . }}
    {{- end }}
    {{- end }}
    {{- if .private }}
    {{- range .homepkgs.private }}
    {{ . }}
    {{- end }}
    {{- end }}
  ];
}

